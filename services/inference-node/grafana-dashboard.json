{
  "dashboard": {
    "title": "Synthetic Intelligence Platform - Production Dashboard",
    "tags": ["medical-ai", "llm", "gpu", "synthetic-intelligence"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "10s",
    "panels": [
      {
        "title": "Request Rate by Agent Type",
        "type": "graph",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(http_requests_total{endpoint=~\".*chat.*\"}[5m])",
            "legendFormat": "{{agent_type}}"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ]
      },
      {
        "title": "Response Latency (p50, p95, p99)",
        "type": "graph",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p99"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "title": "GPU Memory Usage",
        "type": "graph",
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "nvidia_gpu_memory_used_bytes / nvidia_gpu_memory_total_bytes * 100",
            "legendFormat": "GPU {{gpu}} - {{instance}}"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Memory %", "min": 0, "max": 100},
          {"format": "short"}
        ]
      },
      {
        "title": "GPU Utilization",
        "type": "graph",
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "nvidia_gpu_utilization",
            "legendFormat": "GPU {{gpu}} - {{instance}}"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "GPU %", "min": 0, "max": 100},
          {"format": "short"}
        ]
      },
      {
        "title": "Agent Performance (Avg Latency by Tier)",
        "type": "table",
        "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "avg(http_request_duration_seconds_sum) by (agent_type)",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "agent_type": "Agent",
                "Value": "Avg Latency (s)"
              }
            }
          }
        ]
      },
      {
        "title": "Token Generation Rate",
        "type": "stat",
        "gridPos": {"x": 12, "y": 16, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "rate(tokens_generated_total[5m])",
            "legendFormat": "Tokens/sec"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "fields": "",
            "calcs": ["lastNotNull"]
          },
          "text": {},
          "textMode": "auto"
        }
      },
      {
        "title": "Active Concurrent Requests",
        "type": "stat",
        "gridPos": {"x": 18, "y": 16, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "http_requests_inprogress",
            "legendFormat": "Active"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto"
        }
      },
      {
        "title": "Speed Tier Distribution",
        "type": "piechart",
        "gridPos": {"x": 12, "y": 20, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total[5m])) by (speed_tier)",
            "legendFormat": "{{speed_tier}}"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          },
          "pieType": "pie",
          "tooltip": {
            "mode": "single"
          }
        }
      },
      {
        "title": "Error Rate by Agent",
        "type": "graph",
        "gridPos": {"x": 0, "y": 24, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "rate(http_requests_total{status_code=~\"5..\"}[5m])",
            "legendFormat": "{{agent_type}} - 5xx"
          },
          {
            "expr": "rate(http_requests_total{status_code=~\"4..\"}[5m])",
            "legendFormat": "{{agent_type}} - 4xx"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Errors/sec"},
          {"format": "short"}
        ]
      },
      {
        "title": "Model Server Health Status",
        "type": "stat",
        "gridPos": {"x": 12, "y": 24, "w": 12, "h": 4},
        "targets": [
          {
            "expr": "up{job=\"llama-server\"}",
            "legendFormat": "{{instance}}"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "auto",
          "orientation": "horizontal",
          "reduceOptions": {
            "values": false,
            "fields": "",
            "calcs": ["lastNotNull"]
          },
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "title": "Orchestrator Workflows (Parallel Efficiency)",
        "type": "gauge",
        "gridPos": {"x": 12, "y": 28, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "avg(workflow_parallel_efficiency) by (workflow_type)",
            "legendFormat": "{{workflow_type}}"
          }
        ],
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "percentage",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 50, "color": "yellow"},
                {"value": 80, "color": "green"}
              ]
            },
            "min": 0,
            "max": 100,
            "unit": "percent"
          }
        }
      }
    ]
  }
}
